{
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "arrowParens": "always",
  "importOrder": [
    "^react$",
    "^(?!^react$|^@/|^[./]).*",
    "^@/app/(.*)$",
    "^@/config(.*)$",
    "^@/component(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": ["jsx", "typescript"]
}
